{
    "IpRateLimiting": {
        "EnableEndpointRateLimiting": true,
        "StackBlockedRequests": false,
        "RealIpHeader": "X-Real-IP",
        "ClientIdHeader": "X-ClientId",
        "HttpStatusCode": 429,
        "IpWhitelist": [ "127.0.0.1", "::1/10", "192.168.0.0/24" ],
        "EndpointWhitelist": [ ],
        "ClientWhitelist": [ ],
        "GeneralRules": [
            {
                "Endpoint": "*:/chatbot/requestsong",
                "Period": "1s",
                "Limit": 1
            },
            {
                "Endpoint": "*:/chatbot/removesong",
                "Period": "1s",
                "Limit": 1
            },
            {
                "Endpoint": "*:/chatbot/removecurrentsong",
                "Period": "1s",
                "Limit": 1
            },
            {
                "Endpoint": "*:/chatbot/editsong",
                "Period": "1s",
                "Limit": 1
            },
            {
                "Endpoint": "*:/chatbot/promoterequest",
                "Period": "1s",
                "Limit": 1
            },
            {
                "Endpoint": "*:/chatbot/addsongtodrive",
                "Period": "1s",
                "Limit": 1
            },
            {
                "Endpoint": "*:/chatbot/emptyplaylist",
                "Period": "1s",
                "Limit": 1
            },
            {
                "Endpoint": "*:/chatbot/openplaylist",
                "Period": "1s",
                "Limit": 1
            },
            {
                "Endpoint": "*:/chatbot/verycloseplaylist",
                "Period": "1s",
                "Limit": 1
            }
        ]
    }
}
